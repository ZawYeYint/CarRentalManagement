    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="App.razor" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="App.razor">
